        <font>
            <Font name="Comic Sans MS" size="24.0"/>
        </font>
    </Text>
    <Button fx:id="signIn" ellipsisString="" layoutX="507.0" layoutY="243.0" mnemonicParsing="false"
            onAction="#signLoginAction" onMouseEntered="#lightColor" onMouseExited="#darkColor" prefHeight="74.0"
            prefWidth="141.0"
            style="-fx-background-color: #1e90ff; -fx-border-width: 4px; -fx-border-color: #000000; -fx-border-radius: 8px; -fx-background-radius: 12px;"
