        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
        <font>
            <Font name="Comic Sans MS" size="22.0"/>
        </font>
    </Button>
    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="74.0" layoutX="336.0" layoutY="345.0"
               opacity="0.0" stroke="#000000" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="OUTSIDE"
               strokeWidth="5.0" width="141.0"/>
    <Text fontSmoothingType="LCD" layoutX="354.0" layoutY="392.0" strokeLineCap="ROUND" strokeType="OUTSIDE"
          strokeWidth="0.0" text="Find gym">
        <font>
            <Font name="Comic Sans MS" size="24.0"/>
        </font>
    </Text>
    <Button fx:id="findGym" ellipsisString="" layoutX="336.0" layoutY="345.0" mnemonicParsing="false"
            onAction="#findGymAction" onMouseEntered="#lightColor" onMouseExited="#darkColor" prefHeight="74.0"
            prefWidth="141.0"
