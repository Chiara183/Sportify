        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
        <font>
            <Font name="Comic Sans MS" size="22.0"/>
        </font>
    </Button>
    <effect>
      <ColorAdjust contrast="0.05" />
   </effect>
</Pane>
